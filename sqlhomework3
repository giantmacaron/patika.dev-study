1- SELECT country FROM country WHERE country LIKE 'A%a';

2- SELECT country FROM country WHERE country LIKE '%n' and length(country) > 6;

3- SELECT title FROM film WHERE lower(title) LIKE '%t%t%t%t'

4- SELECT * FROM film WHERE title LIKE 'C%' and length > 90 and rental_rate = 2.99
